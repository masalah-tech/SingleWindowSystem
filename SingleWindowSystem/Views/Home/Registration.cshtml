@section Scripts{
    <script>
        sortFormsetIds = ($itemsContainer) => {
            var rows = $itemsContainer.children(".repeater-item")
            console.log("ddd");
            $.each(rows, function (i, row) {
                $(this).children("div").find("input, select").each(function (j, element) {
                    var newString = $(this).attr("name");
                    newString = newString.replace(/\[.*?\]/g, "");
                    $(this).attr("id", "id-" + i + "-" + newString.replace("Forms.", ""));
                    $(this).attr("name", $(this).attr("name").replace(/\[.*?\]/g, `[${i}]`));
                    console.log(newString);
                })
            })
        }

        // controlOtherOptionPopups("proj-reg-primary-proj-data-activity-type");
        // controlOtherOptionPopups("proj-reg-products-form-repeater-1-2");

        // Data tables
        $(document).ready(function () {

            var table = $('.table').DataTable({

                ordering: true,
                info: true,
                lengthMenu: [10, 20, 30, 100],
                columnDefs: [
                    { targets: [0], orderable: true },
                    { targets: "_all", orderable: false }
                ],
                language: {
                    search: '@Search',
                    lengthMenu: '@lengthMenuLeft _MENU_ @lengthMenuRight',
                    info: '@lengthMenuLeft _START_ @to _END_ @from _TOTAL_ @lengthMenuRight',
                    paginate: {
                        first: '@first',
                        previous: '@previous',
                        next: '@next',
                        last: '@last'
                    }
                }
            });


            controleTableBtns();
            function controleTableBtns() {
                const paginateBtns = document.querySelectorAll(".paginate_button");

                paginateBtns.forEach(paginateBtn => {
                    paginateBtn.onclick = () => {
                        updateLang();
                        controleTableBtns();
                    }
                });

                document.querySelectorAll(".sorting").forEach(sortBtn => {
                    sortBtn.onclick = () => {
                        updateLang();
                        controleTableBtns();
                    }
