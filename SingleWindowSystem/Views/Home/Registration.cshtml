@model ProjectRegistrationViewModel

@{
    ViewData["page_title_data_lang"] = "Project Registration";
    int conterForTimeFream = 0;
   

    var lang = Context.Session.GetString("lang");
    string Search = (lang == "en") ? "Search" : "بحث";
    string lengthMenuLeft = (lang == "en") ? "Show" : "عرض الصفوف من";
    string lengthMenuRight = (lang == "en") ? "Entries" : "";
    string to = (lang == "en") ? "to" : "إلى";
    string from = (lang == "en") ? "from" : "من أصل";
    string first = (lang == "en") ? "first" : "الأول";
    string previous = (lang == "en") ? "previous" : "السابق";
    string next = (lang == "en") ? "next" : "التالي";
    string last = (lang == "en") ? "last" : "الأخير";
}

<div class="container-xxl flex-grow-1 container-p-y content-area no-scroll">
    <div class="row container-l2">
        <div class="col-12 container-l3">
            <div class="bs-stepper wizard-numbered container-l4 custom">
                <div class="bs-stepper-header">
                    <div class="step" data-target="#data-about-the-project">
                    <button type="button" class="step-trigger">
                        <span class="bs-stepper-circle">1</span>
                        <span class="bs-stepper-label">
                        <span class="bs-stepper-title lh-base" data-lang="Data About the Project"></span>
                        </span>
                    </button>
                    </div>
                    <div class="line"></div>
                    <div class="step" data-target="#data-about-the-project-owner">
                    <button type="button" class="step-trigger">
                        <span class="bs-stepper-circle">2</span>
                        <span class="bs-stepper-label">
                        <span class="bs-stepper-title lh-base" data-lang="Data About the Project Owner"></span>
                        </span>
                    </button>
                    </div>
                    <div class="line"></div>
                    <div class="step lh-base" data-target="#other-data">
                    <button type="button" class="step-trigger">
                        <span class="bs-stepper-circle">3</span>
                        <span class="bs-stepper-label">
                        <span class="bs-stepper-title" data-lang="Other Data"></span>
                        </span>
                    </button>
                    </div>
                    <div class="order-no">
                        <span data-lang="Order No:"></span>
                        <span>####</span>
                    </div>
                </div>
                <div class="bs-stepper-content c-scroll container-l5">
                    <form class="container-l6 mzn-validation" novalidate>
                    @*<form class="container-l6">*@
                        @*first side start*@
                        <div id="data-about-the-project" class="content container-l7">
                            <div class="row g-3 h-100 position-relative">
                                <div class="container-l8 c-scroll stepper-content-wrapper">
                                    <div class="container-l9 c-scroll">
                                        <div class="row g-3 box-of-inputs first">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Primary Project Data"></h6>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-primary-proj-data-project-name" data-lang="Project Name"></label>
                                                <input required asp-for="projectViewModel.ProjectName_Name" type="text" id="proj-reg-primary-proj-data-project-name" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.ProjectName_Name" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label"  for="proj-reg-primary-proj-data-activity-type" data-lang="Activity Type"></label>
                                                <select required asp-for="projectViewModel.ActivityType" kg="@*@(new SelectList(_unitOfWork.TypeOfActivityRepository.FindAll(a=>a.Deffult==true)," Name","Name"))*@" id="proj-reg-primary-proj-data-activity-type" custom-id="proj-reg-primary-proj-data-activity-type" class="formValidationSelect2 select form-select has-new-options">
                                                    <option value="Select" data-lang="Select Activity Type" selected></option>
                                                    <option value="addNew" data-lang="Others (Must be Specified)"></option>
                                                </select>
                                                <span asp-validation-for="projectViewModel.ActivityType" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-primary-proj-data-order-type" data-lang="Order Type"></label>
                                                <select required asp-for="projectViewModel.TypeOfApplcation" id="proj-reg-primary-proj-data-order-type" custom-id="proj-reg-primary-proj-data-order-type" class="select form-select">
                                                    <option value="1" selected data-lang="New (Establishment)"></option>
                                                    <option value="2" data-lang="Expansion"></option>
                                                    <option value="3" data-lang="Development"></option>
                                                    <option value="4" data-lang="Benefit"></option>
                                                </select>
                                                <span asp-validation-for="projectViewModel.TypeOfApplcation" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-primary-proj-data-activity-description" data-lang="Activity Description"></label>
                                                <textarea required asp-for="projectViewModel.Description"
                                                    id="proj-reg-primary-proj-data-activity-description"
                                                    class="form-control no-resize"
                                                    placeholder="##"
                                                    aria-label="Activity Description"
                                                ></textarea>
                                                <span asp-validation-for="projectViewModel.Description" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                        </div>
                                        <div class="row g-3 box-of-inputs governorate-controls-directorate">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Project Location and Land Area Required for the Project"></h6>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-governorate" data-lang="Governorate"></label>
                                                <select required asp-for="projectViewModel.LandArea_StatePrvice" id="proj-reg-proj-loc-governorate" custom-id="proj-reg-proj-loc-governorate" class="select form-select governorates-select has-new-options" controls-directorate="#proj-reg-proj-loc-directorate">
                                                    <option value="Select" data-lang="--- Select the governorate ---" selected></option>
                                                    <!-- Options are generated from the script "regions.js" -->
                                                </select>
                                                <span asp-validation-for="projectViewModel.LandArea_StatePrvice" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-directorate" data-lang="Directorate"></label>
                                                <select required asp-for="projectViewModel.LandArea_City" id="proj-reg-proj-loc-directorate" custom-id="proj-reg-proj-loc-directorate" class="select form-select directorates-select has-new-options">
                                                    <option value="Select" data-lang="--- Select the directorate ---" selected></option>
                                                    @* Options are generated from the script "regions.js" *@
                                                </select>
                                                <span asp-validation-for="projectViewModel.LandArea_City" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-region" data-lang="Region"></label>
                                                <input required asp-for="projectViewModel.LandArea_PostalCode" id="proj-reg-proj-loc-region" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.LandArea_PostalCode" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-street-1" data-lang="Street 1"></label>
                                                <input required asp-for="projectViewModel.LandArea_StreetAddress1"  id="proj-reg-proj-loc-street-1" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.LandArea_StreetAddress1" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-land-area-num">
                                                    <span data-lang="Area as a Number"></span>
                                                    (<span data-lang="Square Meters"></span>)
                                                </label>
                                                <input required asp-for="projectViewModel.LandArea_AreaInFigures" type="text" id="proj-reg-proj-land-area-num" class="form-control sanitize min-0" value="0" min="0" />
                                                <span asp-validation-for="projectViewModel.LandArea_AreaInFigures" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-land-area-text">
                                                    <span data-lang="Area as Text"></span>
                                                    (<span data-lang="Square Meters"></span>)
                                                </label>
                                                <input required asp-for="projectViewModel.LandArea_AreaInWord" id="proj-reg-proj-land-area-text" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.LandArea_AreaInWord" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-land-area-ownership-type" data-lang="Ownership Type"></label>
                                                <select required asp-for="projectViewModel.LandArea_TypeOfOwnership" class="form-select" id="proj-reg-proj-land-area-ownership-type" custom-id="proj-reg-proj-land-area-ownership-type" aria-label="Ownership Type">
                                                    <option value="0" selected data-lang="Private Ownership"></option>
                                                    <option value="1" data-lang="Country Ownership"></option>
                                                    <option value="2" data-lang="Rent"></option>
                                                </select>
                                                <span asp-validation-for="projectViewModel.LandArea_TypeOfOwnership" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                        </div>
                                        <div class="row g-3 box-of-inputs regions-container governorate-controls-directorate">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Administrative Address"></h6>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-owner-country" data-lang="Country"></label>
                                                <select required asp-for="projectViewModel.AS_Country" id="proj-reg-proj-owner-country" custom-id="proj-reg-proj-owner-country" class="select form-select countries-select controls-gov-and-direc">
                                                    @* Options are generated from the script "regions.js" *@
                                                </select>
                                                <span asp-validation-for="projectViewModel.AS_Country" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-ad-governorate" data-lang="Governorate"></label>
                                                <select required asp-for="projectViewModel.AS_StatePrvice" id="proj-reg-ad-governorate" custom-id="proj-reg-ad-governorate" class="select form-select governorates-select has-new-options" controls-directorate="#proj-reg-ad-directorate">
                                                    <option value="Select" data-lang="--- Select the governorate ---" selected></option>
                                                    <!-- Options are generated from the script "regions.js" -->
                                                </select>
                                                <span asp-validation-for="projectViewModel.AS_StatePrvice" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-ad-directorate" data-lang="Directorate"></label>
                                                <select required asp-for="projectViewModel.AS_City" id="proj-reg-ad-directorate" custom-id="proj-reg-ad-directorate" class="select form-select directorates-select has-new-options">
                                                    <option value="Select" data-lang="--- Select the directorate ---" selected></option>
                                                    @* Options are generated from the script "regions.js" *@
                                                </select>
                                                <span asp-validation-for="projectViewModel.AS_City" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-region" data-lang="Region"></label>
                                                <input required asp-for="projectViewModel.AS_PostalCode" id="proj-reg-proj-loc-region" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.AS_PostalCode" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-street-1" data-lang="Street 1"></label>
                                                <input required asp-for="projectViewModel.AS_StreetAddress1" id="proj-reg-proj-loc-street-1" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.AS_StreetAddress1" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-phone-number" data-lang="Phone Number"></label>
                                                <input required asp-for="projectViewModel.ASPhoneNumber" id="proj-reg-proj-loc-phone-number" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.ASPhoneNumber" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-fax" data-lang="Fax"></label>
                                                <input required asp-for="projectViewModel.ASFaxNumber" id="proj-reg-proj-loc-fax" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.ASFaxNumber" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-cellular" data-lang="Cellular"></label>
                                                <input required asp-for="projectViewModel.ASMobileNumber" id="proj-reg-proj-loc-cellular" class="form-control" placeholder="##" />
                                                <span asp-validation-for="projectViewModel.ASMobileNumber" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-mail-box" data-lang="Mail Box"></label>
                                                <input required asp-for="projectViewModel.ASPOBox" id="proj-reg-proj-loc-mail-box" class="form-control" value="00000" />
                                                <span asp-validation-for="projectViewModel.ASPOBox" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                            <div class="col-md-6 mzn-input-container">
                                                <label class="form-label" for="proj-reg-proj-loc-email" data-lang="Email"></label>
                                                <input required asp-for="projectViewModel.ASEmailAddress" id="proj-reg-proj-loc-email" class="form-control" placeholder="firm@@xyz.com" />
                                                <span asp-validation-for="projectViewModel.ASEmailAddress" class="text-danger"></span>
                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                            </div>
                                        </div>
                                        <div class="box-of-inputs">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Project Implementation Timeline"></h6>
                                            </div>
                                            <div class="timeline-boxes-wrapper">
                                               @* @foreach (var timeFream in timeframeTypes)
                                                {*@
                                                <div class="sub-box-of-inputs timeline-box">
                                                    <div class="content-header">
                                                        <span class="d-block" data-lang="@*@timeFream.Name*@"></span>   
                                                    </div>
                                                    <div>
                                                        <input asp-for="timeframeViewModels[conterForTimeFream].TypeOfTimeframeID" value="@*@timeFream.Id*@" type="hidden" />
                                                        <div>
                                                            <div class="d-flex flex-column gap-1">
                                                                <div class="mzn-input-container">
                                                                    <label class="form-label" for="proj-reg-timeline-frame-1-from" data-lang="From"></label>
                                                                    <input required asp-for="timeframeViewModels[conterForTimeFream].DateOfStart" id="proj-reg-timeline-frame-1-from" class="form-control" />
                                                                    <span asp-validation-for="timeframeViewModels[conterForTimeFream].DateOfStart" class="text-danger"></span>
                                                                    <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                                </div>
                                                                <div class="mzn-input-container">
                                                                    <label class="form-label" for="proj-reg-timeline-frame-1-to" data-lang="To"></label>
                                                                    <input required asp-for="timeframeViewModels[conterForTimeFream].DateOfEnd" id="proj-reg-timeline-frame-1-to" class="form-control" />
                                                                    <span asp-validation-for="timeframeViewModels[conterForTimeFream].DateOfEnd" class="text-danger"></span>
                                                                    <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="mt-2 mzn-input-container">
                                                            <label class="form-label" for="proj-reg-timeline-frame-1-notes" data-lang="Notes"></label>
                                                            <textarea required asp-for="timeframeViewModels[conterForTimeFream].Remarks"
                                                                        id="proj-reg-timeline-frame-1-notes"
                                                                        class="form-control no-resize"
                                                                        placeholder="##"
                                                                        aria-label="Notes"></textarea>
                                                            <span asp-validation-for="timeframeViewModels[conterForTimeFream].Remarks" class="text-danger"></span>
                                                            <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                  @*  conterForTimeFream += 1;
                                                }*@
                                            </div>
                                        </div>
                                        <div class="row g-3 box-of-inputs">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Products and Services Offered by the Project"></h6>
                                            </div>
                                            <div class="form-repeater" id="form-repeater">
                                                <div class="items-container">
                                                    <div class="repeater-item">
                                                        <div class="row">
                                                            <div class="mzn-input-container mb-3 col-lg-6 col-xl-4 col-12 mb-0 d-flex flex-column justify-content-between">
                                                                <label class="form-label" for="proj-reg-products-form-repeater-1-1" data-lang="Product / Service"></label>
                                                                <input type="text" required name="productViewModel[].ProductOrService" id="proj-reg-products-form-repeater-1-1" class="form-control" placeholder="##" />
                                                                <span name="productViewModel[].ProductOrService" class="text-danger"></span>
                                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                            </div>
                                                        <div class="mb-3 col-lg-6 col-xl-3 col-12 mb-0 d-flex flex-column justify-content-between mzn-input-container">
                                                            <label class="form-label" for="proj-reg-products-form-repeater-1-2" data-lang="Annual Production Capacity (Unit)"></label>
                                                                <select required name="productViewModel[].UintName" id="proj-reg-products-form-repeater-1-2" custom-id="proj-reg-products-form-repeater-1-2" class="select form-select has-new-options">
                                                                <option value="Select" data-lang="Select Uint" selected></option>
                                                                <option value="addNew" data-lang="Others (Must be Specified)"></option>
                                                            </select>
                                                            <span name="productViewModel[].UintName" class="text-danger"></span>
                                                            <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                        </div>
                                                        <div class="mb-3 col-lg-6 col-xl-3 col-12 mb-0 d-flex flex-column justify-content-between mzn-input-container">
                                                            <label class="form-label" for="proj-reg-products-form-repeater-1-3" data-lang="Annual Production Capacity (Quantity)"></label>
                                                            <input required type="number" name="productViewModel[].Quantity" id="proj-reg-products-form-repeater-1-3" class="form-control sanitize min-0" value="0" min="0" />
                                                            <span name="productViewModel[].Quantity" class="text-danger"></span>
                                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                        </div>
                                                        <div class="mb-3 col-lg-12 col-xl-2 col-12 d-flex align-items-end mb-0 d-none delete-btn-container">
                                                            <button type="button" class="btn btn-label-danger mt-4 remove-row-btn">
                                                                <i class="bx bx-x me-1"></i>
                                                                <span class="align-middle" data-lang="Delete"></span>
                                                            </button>
                                                        </div>
                                                        </div>
                                                        <hr>
                                                    </div>
                                                </div>
                                                <div class="mb-0">
                                                    <button type="button" class="btn btn-primary add-row-btn">
                                                        <i class="bx bx-plus me-1"></i>
                                                        <span class="align-middle" data-lang="Add"></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row g-3 box-of-inputs calculation-container" id="proj-invest-cost">
                                            <div class="content-header mt-0 mb-1">
                                                <h6 class="mb-0" data-lang="Data and Indicators (Project Investment Cost)"></h6>
                                            </div>
                                            <div class="form-repeater">
                                                <div class="items-container">
                                                    <div class="repeater-item">
                                                        <div class="row align-items-start">
                                                            <div class="mb-3 col-lg-4 col-12 mb-0 mzn-input-container">
                                                                <label class="form-label" for="proj-reg-investment-cost-form-repeater-1-1" data-lang="Statement"></label>
                                                                <select required name="costViewModel[].TypeOfCostName" kg="@*@typeOFCost*@" class="form-select has-new-options" id="proj-reg-investment-cost-form-repeater-1-1" custom-id="proj-reg-investment-cost-form-repeater-1-1" aria-label="Statement">
                                                                    <option value="Select" data-lang="Select Type Of Cost" selected></option>
                                                                    <option value="Test 1" data-lang="Test 1"></option>
                                                                    <option value="Test 2" data-lang="Test 2"></option>
                                                                    <option value="Test 3" data-lang="Test 3"></option>
                                                                    <option value="" data-lang="Other Costs (Must be Specified)"></option>
                                                                </select>
                                                                <label class="form-label d-none" for="proj-reg-investment-cost-form-repeater-1-2"></label>
                                                                <span name="costViewModel[].TypeOfCostName" class="text-danger"></span>
                                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                            </div>
                                                            <div class="mb-3 col-lg-6 col-12 mb-0 mzn-input-container">
                                                                <label class="form-label" for="proj-reg-investment-cost-form-repeater-1-3" data-lang="Cost in USD"></label>
                                                                <input type="number" required name="costViewModel[].Amount" id="proj-reg-investment-cost-form-repeater-1-3" class="form-control operand" cal-container="#proj-invest-cost" value="0" min="0" />
                                                                 <span name="costViewModel[].Amount" class="text-danger"></span>
                                                                <span class="mzn-validate-err d-none" data-lang="Please fill out this field"></span>
                                                            </div>

